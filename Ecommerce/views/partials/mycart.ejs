<br />

<div class="card" style="width: 18rem" ">
  <img
    class="card-img-top img-fluid"
    src="<%=data.images %>"
    alt="Card image cap"
    onclick="popup(`<%=data.id%>`,`<%=data.name%>`,`<%=data.description%>`,`<%=data.price%>`,`<%=data.images%>`)"
  />

  <div class="card-body">
    <h5 class="card-title"><%=data.name %></h5>

    <a class="btn btn-primary" href="/addcart/<%=data.id%>" >+  </a>
    <span><%=cart[data.id].quantity %> </span>
    <a  class="btn btn-primary" href="/minuscart/<%=data.id%>">-</a>
    <br>
    <a href="/deletecart/<%=data.id%>" class="btn btn-primary">Delete product </a>
    <a href="/product/<%=data.id%>" class="btn btn-primary">View Details </a>
   
  </div>
</div>
